import java.util.*;
public class Main {
	
	public static void main(String[] args) {
		Scanner s =  new Scanner(System.in);
        int n, sqrt=1,i;
        n = s.nextInt();
        for(i=1;i*i<=n;i++)
        {
            if(i*i>n)
            break;
            
        }
        System.out.println(i-1);

	}
}
