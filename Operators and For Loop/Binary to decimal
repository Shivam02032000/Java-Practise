import java.util.*;
public class Main {
    
	public static void main(String[] args) {
        Scanner s = new Scanner(System.in);
        int b=0,n,r,t=1;
        n = s.nextInt();
        while(n>=0)
        {
            r = n%10;
            b = b + r*t;
            t = 2*t;
            n=n/10;
        }
    System.out.print(b);
	
		

	}
}
