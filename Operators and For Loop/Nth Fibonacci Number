import java.util.*;
public class Solution {


	public static void main(String[] args) {
	Scanner s = new Scanner(System.in);
        int n= s.nextInt(); 
        int n1= 1, n2=1;
        int sum;
        for(int i=1; i<n;i++)
        {
             sum = n1+n2;
            n1=n2;
        n2 = sum;
        }
        
        System.out.println(n1);
		
	}

}
