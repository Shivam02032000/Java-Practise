import java.util.*;
public class Main {
	
	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
        int x = s.nextInt();
        
        for(int i=1;i<=x;i++)
        {
            
            int term = (3*i) + 2;
            
            if(term%4!=0)
            {
                System.out.print(term+" ");
            }
            else
            x=x+1;
        }

	}
}
