
public class Solution {
	
	public static boolean checkMember(int n){
				
		int t1=0, t2=1, nextnum;
        while(t1<n)
        {
            nextnum = t1+t2;
            t1 = t2;
            t2 = nextnum;
        }
        if(t1==n)
        {
            return true;
        }
        else
        {
            return false;
        }
	}
	

}
