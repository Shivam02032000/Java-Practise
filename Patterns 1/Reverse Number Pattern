import java.util.*;
public class Solution {


	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
        int n = s.nextInt();
        for(int i=1;i<=n;i++)
        {
            int startingNumber = i;
            for(int j=1;j<=i;j++)
            {
                System.out.print(startingNumber);
                startingNumber--;
            }
            System.out.println();
        }

		
	}

}

